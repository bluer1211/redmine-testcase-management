<h2><%=l(:permission_edit_test_plan)%> <%= @test_plan.name %></h2>

<%= form_with model: @test_plan, url: project_test_plan_path do |f| %>
  <%= render partial: 'test_plans/form', locals: {form: f} %>
  <%= f.submit l(:button_update) %>
<% end %>

<% content_for :sidebar do %>
  <%= render :partial => 'test_plans/sidebar' %>
<% end %>
