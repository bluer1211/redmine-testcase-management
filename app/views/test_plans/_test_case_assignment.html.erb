<p>
  <%= form.label :test_case %>
  <%= form.text_field :test_case_id, size: 10, required: true, id: 'test_case_id' %>
  <%= submit_tag l(:button_add) %>
  <%= link_to_function l(:button_cancel), '$("#assign-test-case-form").hide();' %>
</p>

<%= javascript_tag "observeAutocompleteField('test_case_id', '#{escape_javascript project_auto_complete_test_cases_path(project_id: @project, test_plan_id: @test_plan.id)}')" %>
